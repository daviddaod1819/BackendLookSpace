<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

        <property name="autoIncrement" value="true"/>

    <!--
        Editing the entity Space.
    -->
    <changeSet id="20210216130606" author="jhipster">
        <addColumn  tableName="space"> 
            <column name="place" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="bathrooms" type="integer">
                <constraints nullable="true" />
            </column>
            <column name="photos" type="varchar(100)">
                <constraints nullable="true" />
            </column>
        </addColumn>
        <modifyDataType tableName="space" columnName="price" newDataType="double"/>
    </changeSet>

</databaseChangeLog>
